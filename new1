//Generate random matrix

	int i, j, row, column;
  srand(time(NULL));

  printf("Enter the row of Matrix: ");
  scanf("%d", &row);
  printf("Enter the column of Matrix: ");
  scanf("%d", &column);

  printf("\n");

  int matrix[row][column];
  
  for(i = 0; i < row; i++){
    for(j = 0; j < column; j++){
      //generating a matrix with random values
      matrix[i][j] = rand()%100;
      printf("%d\t",matrix[i][j]);
    }
    printf("\n");
  }
